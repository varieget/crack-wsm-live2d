{
  "root": true,
  "env": {
    "es6": true,
    "node": true
  },
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "sourceType": "module",
    "requireConfigFile": false
  },
  "extends": ["eslint:recommended", "prettier"],
  "rules": {
    "no-unused-vars": "off",
    "indent": ["error", 2]
  }
}
